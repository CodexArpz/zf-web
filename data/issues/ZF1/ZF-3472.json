        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/39003",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=yoghi",
                        "name": "yoghi",
                        "displayName": "Stefano Tamagnini",
                        "active": true
                    },
                    "body": "REFERENCE ZF 1.10.1\r\ntry: \r\n{code}\r\n$request->setParam('variable',null);\r\nunset($_GET['variable']);\r\nunset($_POST['variable']);\r\n{code}\r\ncorrect unset variable into Zend_Controller_Request_Http, because into Zend_Controller_Request_Http on getParam(...)\r\nit try to find variable name into $_GET & $_POST :\r\n{code}\r\n    public function getParam($key, $default = null)\r\n    {\r\n        $keyName = (null !== ($alias = $this->getAlias($key))) ? $alias : $key;\r\n\r\n        $paramSources = $this->getParamSources();\r\n        if (isset($this->_params[$keyName])) {\r\n            return $this->_params[$keyName];\r\n        } elseif (in_array('_GET', $paramSources) && (isset($_GET[$keyName]))) {\r\n            return $_GET[$keyName];\r\n        } elseif (in_array('_POST', $paramSources) && (isset($_POST[$keyName]))) {\r\n            return $_POST[$keyName];\r\n        }\r\n\r\n        return $default;\r\n    }\r\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=yoghi",
                        "name": "yoghi",
                        "displayName": "Stefano Tamagnini",
                        "active": true
                    },
                    "created": "2010-03-05T12:15:08.000+0000",
                    "updated": "2010-03-05T12:15:08.000+0000"
