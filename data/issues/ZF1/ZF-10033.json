                    "updated": "2010-06-24T12:44:20.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/41183",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=freak",
                        "name": "freak",
                        "displayName": "Dolf Schimmel (Freeaqingme)",
                        "active": true
                    },
                    "body": "I keep getting this error:\r\n{code}1) Zend_Application_Resource_DbTest::testSetDefaultMetadataCache\r\nZend_Cache_Exception: file Zend\/Cache\/Backend\/Blackhole.php not found in include_path\r\n\r\n\/srv\/projects\/ZF\/trunk\/library\/Zend\/Cache.php:209\r\n\/srv\/projects\/ZF\/trunk\/library\/Zend\/Cache.php:148\r\n\/srv\/projects\/ZF\/trunk\/library\/Zend\/Cache.php:94\r\n\/srv\/projects\/ZF\/trunk\/tests\/Zend\/Application\/Resource\/DbTest.php:164\r\n\/srv\/projects\/ZF\/trunk\/tests\/Zend\/Application\/Resource\/DbTest.php:55\r\n\/srv\/projects\/ZF\/trunk\/tests\/Zend\/Application\/Resource\/DbTest.php:214\r\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=freak",
                        "name": "freak",
                        "displayName": "Dolf Schimmel (Freeaqingme)",
                        "active": true
                    },
                    "created": "2010-06-24T17:57:44.000+0000",
