            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/19500",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "No, this is not a problem...\nYou said that you want to have the ISO Year instead of the real year.\n\nThis is normally used when you want to calculate calendars.\nWeek 1 of the Calendarsheet 2008 begins with 31.December.\n\nIf you need to have the real year, you should define it within your userformat.\n\n\"Y\" = ISO-Year\n\"y\" = Real-Year",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
