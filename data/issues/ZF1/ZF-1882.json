            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/18429",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "This leads to another problem:\n\nWhen the locale of your environment is set to another value than 'en' the validation will not work anymore.\n\nIf you define 1000 as integer and cast it to an string you will get '1.000' when 'de' is set as locale or '1 000' if your locale is set to 'fr'.\n\nYou regex will not work in such environments...\n\nThe simplest solution would be to use Zend_Locale_Format::isInteger($number) for such cases... or as fallback to intval(). \n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2008-01-22T04:29:33.000+0000",
                    "updated": "2008-01-22T04:29:33.000+0000"
                },
