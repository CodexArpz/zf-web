                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=toocan",
                        "name": "toocan",
                        "displayName": "Andr\u00e9 Schade",
                        "active": true
                    },
                    "body": "without that part\n\n{code}\n\n if ($inflector->getRules('context') == false) {\n            $inflector\n                ;\n        }\n\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=toocan",
