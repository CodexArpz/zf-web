        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40641",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "Actually you can not assign a cache by option as it is added statically.\r\nThis is the first things which have to be added for this issue.\r\n\r\nTherefor reassigning the issue to myself.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2010-05-22T02:37:49.000+0000",
                    "updated": "2010-05-22T02:37:49.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40884",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "Feature implemented.\r\n\r\nYou can now set the cache within the options of Zend_Translate and it's adapters.\r\n\r\nFeel free to propose any further improvement to the propert component.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2010-06-06T14:57:49.000+0000",
                    "updated": "2010-06-06T14:57:49.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40885",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=paryankkansara",
                        "name": "paryankkansara",
                        "displayName": "Paryank Kansara",
                        "active": true
                    },
                    "body": "Thanks for implementing this feature.\r\n\r\nIt would be nice if we could also assign cache manager template name as an argument to Zend_Translate_Adapter::setCache() because, we cannot define instance of Zend_Cache_Core in configuration file. So defining cache manager template name should automatically assign cache instance to it.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=paryankkansara",
                        "name": "paryankkansara",
                        "displayName": "Paryank Kansara",
                        "active": true
                    },
                    "created": "2010-06-06T16:05:34.000+0000",
                    "updated": "2010-06-06T16:05:34.000+0000"
                }
            ]
        }
