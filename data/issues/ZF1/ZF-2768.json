                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=darby",
                        "name": "darby",
                        "displayName": "Darby Felton",
                        "active": true
                    },
                    "body": "I can also reproduce the failures by running {{phpunit Zend_Controller_Response_HttpTest}}, see [SVN r8477|http:\/\/framework.zend.com\/fisheye\/changelog\/Zend_Framework\/?cs=8477].",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=darby",
                        "name": "darby",
                        "displayName": "Darby Felton",
                        "active": true
