        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/15293",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mlange",
                        "name": "mlange",
                        "displayName": "Marcus Lange",
                        "active": true
                    },
                    "body": "public function addTypes($types)\n{\n    if ($types instanceof DomDocument) {\n        $dom = $this->dom->importNode($types->documentElement, true);\n        $this->wsdl->appendChild($dom);\n     } elseif ($types instanceof DomNode || $types instanceof DomElement || $types instanceof DomDocumentFragment ) {\n        $dom = $this->dom->importNode($types, true);\n        $this->wsdl->appendChild($dom);\n     }\n}\n\nHere is the fix.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mlange",
                        "name": "mlange",
                        "displayName": "Marcus Lange",
                        "active": true
                    },
                    "created": "2007-06-18T20:14:34.000+0000",
                    "updated": "2007-06-18T20:14:34.000+0000"
                },
