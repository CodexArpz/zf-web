                    "body": "The example above is a bit confusing!\r\n\r\n{code:title=Example}\r\n$xmlString = <<<EOT\r\n<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\r\n<config>\r\n    <tables name=\"employees\" \/>\r\n    <tables name=\"vacations\" \/>\r\n<\/config>\r\nEOT;\r\n \r\n$config = new Zend_Config_Xml($xmlString);\r\n\r\nZend_Debug::dump($config->toArray(), 'First test:');\r\n\r\n$xmlString = <<<EOT\r\n<?xml version=\"1.0\" encoding=\"UTF-8\" ?>\r\n<config>\r\n    <test>\r\n        <tables name=\"employees\" \/>\r\n        <tables name=\"vacations\" \/>\r\n    <\/test>\r\n<\/config>\r\nEOT;\r\n \r\n$config = new Zend_Config_Xml($xmlString);\r\n\r\nZend_Debug::dump($config->toArray(), 'Second test:');\r\n{code}\r\n\r\n{code:title=Output|borderStyle=solid}\r\nFirst test: array(1) {\r\n  [\"tables\"] => array(1) {\r\n    [\"name\"] => string(9) \"employees\"\r\n  }\r\n}\r\n\r\nSecond test: array(1) {\r\n  [\"test\"] => array(1) {\r\n    [\"tables\"] => array(2) {\r\n      [0] => array(1) {\r\n        [\"name\"] => string(9) \"employees\"\r\n      }\r\n      [1] => array(1) {\r\n        [\"name\"] => string(9) \"vacations\"\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n{code}",
