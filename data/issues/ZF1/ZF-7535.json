        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/33310",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "Point is defined as 1 to 0.00035.\nPoint_Adobe is defined as 1 to 0.000 352 777 777 78.\n\nSo when you want to calculate postscript (which uses the measurement from adobe) then you must also use POINT_ADOBE and not POINT (which declares the brittain point unit for length and not the computional length)",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2009-08-08T09:17:50.000+0000",
                    "updated": "2009-08-08T09:17:50.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/33319",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=exceptione",
                        "name": "exceptione",
                        "displayName": "exceptione",
                        "active": true
                    },
                    "body": "Thanks for pointing this out, I am sorry to raise a non-issue.\nMaybe we should add the comment that POINT_ADOBE stands for the \"Postscript point\" or \"desktop publishing point\".",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=exceptione",
                        "name": "exceptione",
                        "displayName": "exceptione",
                        "active": true
                    },
