            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40836",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=freak",
                        "name": "freak",
                        "displayName": "Dolf Schimmel (Freeaqingme)",
                        "active": true
                    },
                    "body": "How can one reproduce the issue?",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=freak",
                        "name": "freak",
                        "displayName": "Dolf Schimmel (Freeaqingme)",
                        "active": true
                    },
                    "created": "2010-06-02T17:05:22.000+0000",
                    "updated": "2010-06-02T17:05:22.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40838",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=haruka",
                        "name": "haruka",
                        "displayName": "Sandra Schneider",
                        "active": true
                    },
                    "body": "I got this error while calling:\r\n{code}<?php echo $this->translate(\"translation string\", $someArray); ?>{code}\r\n\r\n$someArray is an array with two elements, 'cause the translation in my po-file has two placeholders.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=haruka",
                        "name": "haruka",
                        "displayName": "Sandra Schneider",
                        "active": true
                    },
                    "created": "2010-06-02T17:45:41.000+0000",
                    "updated": "2010-06-02T17:45:41.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40842",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "Is $this->translate an object of Zend_Translate?\r\nOr is $this a view helper?\r\n\r\nBecause Zend_Translate does not support view operations and the second parameter for Zend_Translate is the language and not a placeholder.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2010-06-03T00:35:06.000+0000",
                    "updated": "2010-06-03T00:35:06.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40846",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=haruka",
                        "name": "haruka",
                        "displayName": "Sandra Schneider",
                        "active": true
                    },
                    "body": "$this is an instance of Zend_View.\r\nSo $this->translate() calls the view helper.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=haruka",
                        "name": "haruka",
                        "displayName": "Sandra Schneider",
                        "active": true
                    },
                    "created": "2010-06-03T02:28:46.000+0000",
                    "updated": "2010-06-03T02:28:46.000+0000"
                },
