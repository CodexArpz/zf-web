            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/43186",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=jpieper",
                        "name": "jpieper",
                        "displayName": "Jan Pieper",
                        "active": true
                    },
                    "body": "This isn't a bug. You need to add \"null\" to the @param tag.\r\n\r\n{code:title=fails}\/**\r\n * @param array $list\r\n * @return string\r\n *\/\r\npublic function doSomething($list = null) {\r\n    \/\/ ...\r\n}{code}\r\n\r\n{code:title=works fine}\/**\r\n * @param array|null $list\r\n * @return string\r\n *\/\r\npublic function doSomething($list = null) {\r\n    \/\/ ...\r\n}{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=jpieper",
                        "name": "jpieper",
                        "displayName": "Jan Pieper",
                        "active": true
                    },
