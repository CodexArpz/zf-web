        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/17854",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "2 things to beware of:\n\nFirst:\nCSV files are not intended to be \"human readable\" in a stylish way.\nWhen whitespaces appear then they will also be included in any CSV import.\nThis would f.e. break excel, word and any other software you or your customer are using for editing this, except a plain text editor.\n\nSecond:\nTranslation means that the text is returned as it is. There must be no formatting, striooing and so on. Sometimes whitespaces are necessary and should be returned.\n\nThe aproved way for this is to write a small helper which does this for you within the view.\nZend_Translate will not truncate or change any output it reads. This is part of the view.\n\nTherefor this improvement will not be integrated.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2007-12-11T08:25:36.000+0000",
                    "updated": "2007-12-11T08:25:36.000+0000"
                }
            ]
        }
