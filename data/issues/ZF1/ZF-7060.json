            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/32025",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=cadorn",
                        "name": "cadorn",
                        "displayName": "Christoph Dorn",
                        "active": true
                    },
                    "body": "I am currently working on expanding the features and API of Zend_Wildfire. Once this is done I will be including full documentation.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=cadorn",
                        "name": "cadorn",
                        "displayName": "Christoph Dorn",
                        "active": true
                    },
                    "created": "2009-06-20T15:22:44.000+0000",
                    "updated": "2009-06-20T15:22:44.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42464",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=cadorn",
                        "name": "cadorn",
                        "displayName": "Christoph Dorn",
                        "active": true
                    },
                    "body": "This needs to be discussed in light of ZF 2.\r\n\r\nThe original goal was to provide only support for the log writer and DB profiler. There is a lot of room for expansion to include other components.\r\n\r\nZend_Wildfire needs to be refactored slightly to take advantage of new features developed with FirePHP 1.0. I would want to do the refactoring before documenting the API. This was always planned which is why documentation was omitted.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=cadorn",
                        "name": "cadorn",
                        "displayName": "Christoph Dorn",
                        "active": true
                    },
                    "created": "2010-10-09T17:34:27.000+0000",
                    "updated": "2010-10-09T17:34:27.000+0000"
                }
            ]
