                    "body": "Decorating the Zend_Form_Element_Captcha seems almost imposible. Please fix this issue!",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=gotys",
                        "name": "gotys",
                        "displayName": "Petr Gottvald",
                        "active": true
                    },
                    "created": "2010-05-06T09:04:08.000+0000",
                    "updated": "2010-05-06T09:04:08.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40887",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alab",
                        "name": "alab",
                        "displayName": "Christian Albrecht",
                        "active": true
                    },
                    "body": "{code}\r\n$form = new Zend_Form();\r\n$form->addElement('captcha', 'CaptchaName',\r\n\t\t  array('captcha' => new Zend_Captcha_Figlet(),\r\n                        'prefixPath' => array('decorator' => array('App_Form_Decorator' => 'App\/Form\/Decorator\/'))));\r\n$form->render();\r\necho '<pre>' . var_export(array_keys($form->CaptchaName->getDecorators()),1) . '<\/pre>';\r\n\r\narray (\r\n  0 => 'App_Form_Decorator_Captcha',\r\n  1 => 'App_Form_Decorator_Captcha_Word',\r\n  2 => 'Zend_Form_Decorator_Errors',\r\n  3 => 'Zend_Form_Decorator_Description',\r\n  4 => 'Zend_Form_Decorator_HtmlTag',\r\n  5 => 'Zend_Form_Decorator_Label',\r\n)\r\n{code}\r\n\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alab",
                        "name": "alab",
                        "displayName": "Christian Albrecht",
                        "active": true
                    },
                    "created": "2010-06-07T00:30:10.000+0000",
                    "updated": "2010-06-07T00:30:10.000+0000"
                },
                {
