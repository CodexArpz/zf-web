            "value": [
                {
                    "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/comment\/11115",
                    "author": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "body": "Functionality like this was actually removed from the original code submission. While convenient, it is a dangerous practice as it allows unfiltered input to be used to instantiate and populate a user-defined class.\n\nYou may either tell Zend_Json_Decoder to create a stdclass object (just a glorified associative array) or an associative array when decoding JSON objects by passing the $decodeType to Zend_Json::decode() or Zend_Json_Decoder::__construct().",
                    "updateAuthor": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "created": "2006-11-02T01:37:14.000+0000",
                    "updated": "2006-11-02T01:37:14.000+0000"
                },
                {
