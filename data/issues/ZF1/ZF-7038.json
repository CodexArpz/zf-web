        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/31943",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mben",
                        "name": "mben",
                        "displayName": "Marc Bennewitz (GIATA mbH)",
                        "active": true
                    },
                    "body": "Patch for Zend_Http_Client::getParametersRecursive\n{code}\n    protected function _getParametersRecursive($parray, $urlencode = false)\n    {\n        if (!is_array($parray)) {\n            return $parray;\n        }\n\n        $parameters = array();\n        $query = http_build_query($parray, '', '&');\n        foreach (explode('&', $query) as $qpart) {\n            list($pname, $pval) = explode('=', $qpart);\n            if (!$urlencode) {\n                $pname = urldecode($pname);\n                $pval  = urldecode($pval);\n            }\n            $parameters[] = array($pname, $pval);\n        }\n\n        return $parameters;\n    }\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mben",
                        "name": "mben",
                        "displayName": "Marc Bennewitz (GIATA mbH)",
                        "active": true
                    },
                    "created": "2009-06-18T01:23:41.000+0000",
