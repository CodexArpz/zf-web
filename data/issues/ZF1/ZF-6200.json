            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/32106",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=nickpack",
                        "name": "nickpack",
                        "displayName": "Nick Pack",
                        "active": true
                    },
                    "body": "A quick workaround for this is to add:\n\n{quote}\nif (array_key_exists('name', $attribs)) {\n    unset($attribs['name']);\n}\n\n{quote}\n\nto Zend_View_Helper_Form::form() somewhere before the $xhtml variable is defined.\n\nNot a particularly good workaround, but fixes validation issues\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=nickpack",
                        "name": "nickpack",
                        "displayName": "Nick Pack",
                        "active": true
                    },
                    "created": "2009-06-23T07:48:33.000+0000",
                    "updated": "2009-06-23T07:48:33.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/32108",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=nickpack",
                        "name": "nickpack",
                        "displayName": "Nick Pack",
                        "active": true
                    },
                    "body": "Apologies, didnt think JIRA would try to parse it!\n\nif (array_key_exists('name', $attribs)) {\n            unset($attribs['name']);\n }",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=nickpack",
                        "name": "nickpack",
                        "displayName": "Nick Pack",
                        "active": true
