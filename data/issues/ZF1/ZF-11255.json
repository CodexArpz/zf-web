            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/45789",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ashireman",
                        "name": "ashireman",
                        "displayName": "Anthony Shireman",
                        "active": true
                    },
                    "body": "Create a new project. Add the Zend Framework library. Replace the \/application folder with the contents of the zip file. The only files changed from the default are Bootstrap.php and the view script for the index controller, index action",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ashireman",
                        "name": "ashireman",
                        "displayName": "Anthony Shireman",
                        "active": true
                    },
                    "created": "2011-04-02T23:58:45.000+0000",
                    "updated": "2011-04-02T23:58:45.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/45790",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ashireman",
                        "name": "ashireman",
                        "displayName": "Anthony Shireman",
                        "active": true
                    },
                    "body": "Duplicate of http:\/\/framework.zend.com\/issues\/browse\/ZF-6486\r\n\r\nThe current behavior is not clear in the documentation. If you use any routes in your application all calls to the url helper must be changed to the following:\r\n$this->view->url(array('module' => 'account', 'controller' => 'index', 'action' => 'login'), ***'default'***, false);\r\n\r\nfrom this:\r\n$this->view->url(array('module' => 'account', 'controller' => 'index', 'action' => 'login'), null, false);\r\n\r\nas the expectation changes from null being the defined default route to the explicit 'default'.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ashireman",
                        "name": "ashireman",
                        "displayName": "Anthony Shireman",
