                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/39955",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=raspi",
                        "name": "raspi",
                        "displayName": "Pekka J\u00e4rvinen",
                        "active": true
                    },
                    "body": "Now I tested with core PDO and it returns correct result so it's not PDO issue.\r\n\r\n{code}\r\n    $select = $this->select()->from($this, array('m' => 'MAX(custord)'));\r\n    $select->group(array('custord'));\r\n    $select->order(array('custord DESC'));\r\n\r\n    $sql = $select->__toString();\r\n\r\n    $db = new PDO(\"pgsql:host=127.0.0.1;dbname=quux\", 'foo', 'bar');\r\n    foreach($db->query($sql) as $res)\r\n    {\r\n      var_dump($res);\r\n    }\r\n\r\n    die;\r\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=raspi",
                        "name": "raspi",
                        "displayName": "Pekka J\u00e4rvinen",
