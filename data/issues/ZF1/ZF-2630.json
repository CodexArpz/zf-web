                    "created": "2008-02-13T14:05:59.000+0000",
                    "updated": "2008-02-13T14:05:59.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/19133",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=pberry@qsigroup.com",
                        "name": "pberry@qsigroup.com",
                        "displayName": "Phillip Berry",
                        "active": true
                    },
                    "body": "Code to reproduce:\n        $form = new Zend_Form;\n        $select = $form->addElement(new Zend_Form_Element_Select('test'));\n        $form->test->addMultiOption('foo','bar');\n        $translate = new Zend_Translate('array',array('foo' => 'FOO');\n        $form->setDefaultTranslator($translate);\n        $this->view->testForm = $form;\n\nExpected Outcome:\n<select name=\"test\" id=\"test\">\n\t<option value=\"bar\" label=\"FOO\">FOO<\/option>\n<\/select>\n\nActual Outcome:\n<select name=\"test\" id=\"test\">\n\t<option value=\"foo\" label=\"FOO\">FOO<\/option>\n<\/select>\n\nPatch for Multi.php:\n@@ -100,7 +100,7 @@\n         $option  = (string) $option;\n         $this->_getMultiOptions();\n         if (null !== ($translator = $this->getTranslator())) {\n-            $value = $translator->translate($option);\n+            $option = $translator->translate($option);\n             $this->_translated[$option] = true;\n         }\n         $this->options[$option] = $value;\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=pberry@qsigroup.com",
                        "name": "pberry@qsigroup.com",
                        "displayName": "Phillip Berry",
                        "active": true
