            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/49393",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "body": "I have added the MySQL description (which works perfectly) instead of the PosgreSQL\r\n\r\n-- Table: devise\r\n\r\n-- DROP TABLE devise;\r\n\r\nCREATE TABLE devise\r\n(\r\n  dev_id integer NOT NULL DEFAULT nextval('devise_id_seq'::regclass),\r\n  codedevise character(3) NOT NULL,\r\n  libelle character varying(20),\r\n  taux numeric(10,5),\r\n  defaut character(1) NOT NULL DEFAULT 'N'::bpchar,\r\n  CONSTRAINT dev_primary PRIMARY KEY (dev_id ),\r\n  CONSTRAINT defaut CHECK (defaut = ANY (ARRAY['N'::bpchar, 'O'::bpchar, 'S'::bpchar]))\r\n)\r\nWITH (\r\n  OIDS=FALSE\r\n);\r\nALTER TABLE devise\r\n  OWNER TO pgbanque;\r\n\r\n-- Index: dev_clealpha\r\n\r\n-- DROP INDEX dev_clealpha;\r\n\r\nCREATE INDEX dev_clealpha\r\n  ON devise\r\n  USING btree\r\n  (libelle COLLATE pg_catalog.\"default\" );\r\n\r\n-- Index: dev_cledevise\r\n\r\n-- DROP INDEX dev_cledevise;\r\n\r\nCREATE UNIQUE INDEX dev_cledevise\r\n  ON devise\r\n  USING btree\r\n  (codedevise COLLATE pg_catalog.\"default\" );\r\n\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "created": "2012-01-02T21:03:44.000+0000",
                    "updated": "2012-01-02T21:03:44.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/49394",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "body": "I have added the MySQL description (which works perfectly) instead of the PosgreSQL\r\n\r\n-- Table: devise\r\n\r\n-- DROP TABLE devise;\r\n\r\nCREATE TABLE devise\r\n(\r\n  dev_id integer NOT NULL DEFAULT nextval('devise_id_seq'::regclass),\r\n  codedevise character(3) NOT NULL,\r\n  libelle character varying(20),\r\n  taux numeric(10,5),\r\n  defaut character(1) NOT NULL DEFAULT 'N'::bpchar,\r\n  CONSTRAINT dev_primary PRIMARY KEY (dev_id ),\r\n  CONSTRAINT defaut CHECK (defaut = ANY (ARRAY['N'::bpchar, 'O'::bpchar, 'S'::bpchar]))\r\n)\r\nWITH (\r\n  OIDS=FALSE\r\n);\r\nALTER TABLE devise\r\n  OWNER TO pgbanque;\r\n\r\n-- Index: dev_clealpha\r\n\r\n-- DROP INDEX dev_clealpha;\r\n\r\nCREATE INDEX dev_clealpha\r\n  ON devise\r\n  USING btree\r\n  (libelle COLLATE pg_catalog.\"default\" );\r\n\r\n-- Index: dev_cledevise\r\n\r\n-- DROP INDEX dev_cledevise;\r\n\r\nCREATE UNIQUE INDEX dev_cledevise\r\n  ON devise\r\n  USING btree\r\n  (codedevise COLLATE pg_catalog.\"default\" );\r\n\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "created": "2012-01-02T21:04:57.000+0000",
                    "updated": "2012-01-02T21:04:57.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/49478",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "body": "Probably found the root cause : ZF automatically generates nextval('devise_dev_id_seq'::regclass) based on <table name>_<autoincrement field name>_seq instead of uusing the actual sequence defined in PostgreSQL. \r\nWork around : changed the sequence name to fit ZF generated name. ",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "created": "2012-01-11T23:38:40.000+0000",
                    "updated": "2012-01-11T23:38:40.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/49479",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=vtap",
                        "name": "vtap",
                        "displayName": "Vincent Tap",
                        "active": true
                    },
                    "body": "Fixed by using \r\nprotected $_sequence = 'devise_id_seq';",
