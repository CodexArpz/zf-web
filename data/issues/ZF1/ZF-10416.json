            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42253",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=patf",
                        "name": "patf",
                        "displayName": "Patrick Figel",
                        "active": true
                    },
                    "body": "For some weird reason, Microsoft SQL Server returns those messages as warnings.\r\nBy default, SQL Server Driver handles warnings like errors.\r\nTo change the behaviour, put the following line in your php.ini:\r\nsqlsrv.WarningsReturnAsErrors = 0\r\n... or use sqlsrv_configure:\r\nsqlsrv_configure(\"WarningsReturnAsErrors\", 0);\r\n\r\nThis can be marked as \"not an issue\".",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=patf",
                        "name": "patf",
