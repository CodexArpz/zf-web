                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/33013",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sojuz",
                        "name": "sojuz",
                        "displayName": "Adam Mielko",
                        "active": true
                    },
                    "body": "Its impossible to add custom captcha element. \n\nmethod setOptions should call its parent before executing any logic\n\npublic function setOptions(array $options)\n{\n        parent::setOptions($options);\n        if (array_key_exists('captcha', $options)) {\n            if (array_key_exists('captchaOptions', $options)) {\n                $this->setCaptcha($options['captcha'], $options['captchaOptions']);\n                unset($options['captchaOptions']);\n            } else {\n                $this->setCaptcha($options['captcha']);\n            }\n            unset($options['captcha']);\n        }\n        return $this;\n}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sojuz",
                        "name": "sojuz",
                        "displayName": "Adam Mielko",
                        "active": true
                    },
