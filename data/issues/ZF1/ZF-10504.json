            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42391",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=dominikgehl",
                        "name": "dominikgehl",
                        "displayName": "Dominik Gehl",
                        "active": true
                    },
                    "body": "The following patch fixes the problem\r\n\r\n\r\n@@ -522,6 +522,9 @@\r\n                 case '[UIDVALIDITY':\r\n                     $result['uidvalidity'] = (int)$tokens[2];\r\n                     break;\r\n+                case '[UIDNEXT':\r\n+                    $result['uidnext'] = (int)$tokens[2];\r\n+                    break;\r\n                 default:\r\n                     \/\/ ignore\r\n             }\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=dominikgehl",
                        "name": "dominikgehl",
                        "displayName": "Dominik Gehl",
                        "active": true
                    },
                    "created": "2010-09-29T09:48:13.000+0000",
                    "updated": "2010-09-29T09:48:13.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42393",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=dominikgehl",
                        "name": "dominikgehl",
                        "displayName": "Dominik Gehl",
                        "active": true
                    },
                    "body": "Here's the patch against a SVN checkout\r\n\r\n--- library\/Zend\/Mail\/Protocol\/Imap.php\t(revision 23006)\r\n+++ library\/Zend\/Mail\/Protocol\/Imap.php\t(working copy)\r\n@@ -517,6 +517,9 @@\r\n                 case '[UIDVALIDITY':\r\n                     $result['uidvalidity'] = (int)$tokens[2];\r\n                     break;\r\n+                case '[UIDNEXT':\r\n+                    $result['uidnext'] = (int)$tokens[2];\r\n+                    break;\r\n                 default:\r\n                     \/\/ ignore\r\n             }\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=dominikgehl",
                        "name": "dominikgehl",
                        "displayName": "Dominik Gehl",
                        "active": true
                    },
                    "created": "2010-09-29T11:04:13.000+0000",
                    "updated": "2010-09-29T11:04:13.000+0000"
                }
            ]
        }
