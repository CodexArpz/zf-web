            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42987",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sylvestre",
                        "name": "sylvestre",
                        "displayName": "Sylvestre Ledru",
                        "active": true
                    },
                    "body": "The following patch fixes the issue:\r\n\r\n--- Html.php\t2010-11-15 19:13:29.000000000 +0100\r\n+++ \/usr\/share\/php\/Zend\/Search\/Lucene\/Document\/Html.php\t2010-11-15 19:12:57.000000000 +0100\r\n@@ -102,7 +102,7 @@\r\n             \/\/ Document encoding is not recognized\r\n \r\n             \/** @todo improve HTML vs HTML fragment recognition *\/\r\n-            if (preg_match('\/<html>\/i', $htmlData, $matches, PREG_OFFSET_CAPTURE)) {\r\n+            if (preg_match('\/<html(\\.*)>\/i', $htmlData, $matches, PREG_OFFSET_CAPTURE)) {\r\n                 \/\/ It's an HTML document\r\n                 \/\/ Add additional HEAD section and recognize document\r\n                 $htmlTagOffset = $matches[0][1] + strlen($matches[0][0]);\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sylvestre",
                        "name": "sylvestre",
                        "displayName": "Sylvestre Ledru",
                        "active": true
                    },
                    "created": "2010-11-15T10:16:02.000+0000",
                    "updated": "2010-11-15T10:16:02.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/42988",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sylvestre",
                        "name": "sylvestre",
                        "displayName": "Sylvestre Ledru",
                        "active": true
                    },
                    "body": "Sorry, the actual patch is (a \\ was too much)\r\n\r\n\u2014 Html.php\t2010-11-15 19:13:29.000000000 +0100\r\n+++ \/usr\/share\/php\/Zend\/Search\/Lucene\/Document\/Html.php\t2010-11-15 19:12:57.000000000 +0100\r\n@@ -102,7 +102,7 @@\r\n\/\/ Document encoding is not recognized\r\n\r\n\/** @todo improve HTML vs HTML fragment recognition *\/\r\n\r\nif (preg_match('\/<html>\/i', $htmlData, $matches, PREG_OFFSET_CAPTURE)) {\r\n+ if (preg_match('\/<html(.*)>\/i', $htmlData, $matches, PREG_OFFSET_CAPTURE)) {\r\n\/\/ It's an HTML document\r\n\/\/ Add additional HEAD section and recognize document\r\n$htmlTagOffset = $matches[0][1] + strlen($matches[0][0]);",
                    "updateAuthor": {
