                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=aqk",
                        "name": "aqk",
                        "displayName": "A.Q Khan",
                        "active": true
                    },
                    "body": "I checked the PHP Information for PHP Version 5.2.9 .  The Tokeniser Support is enabled.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=aqk",
                        "name": "aqk",
