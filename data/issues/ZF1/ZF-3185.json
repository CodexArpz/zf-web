        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/21530",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "The patch is errorous.\n\nActually the complete API is object based because it's intention was to be used by date math with fluid interface.\n\nHaving same functionality in strict interface could lead to misuse.\nWe will have to look over the cons before we integrate such functionality.\n\nAlso WHEN strict functions beside the actual ones are integrated this has to be done for all generic usable functions.\n\nThe biggest actual NEG is that the API of Zend_Date is already very big because of it's simplicity and usability. The original intention was not to add functionality until the minimum requirements have been raised and we are able to use the new date extension which is available since PHP 5.2.0.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2008-04-27T11:05:42.000+0000",
                    "updated": "2008-04-27T11:05:42.000+0000"
