        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/15480",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=brenton",
                        "name": "brenton",
                        "displayName": "Brenton Alker",
                        "active": true
                    },
                    "body": "Seems it is simple error in the Regex to separate the parts of the function name.\n\nThe regex\n{code}\n\/^findParent(\\w+)(?:By(\\w+))?$\/\n{code}\n\nperforms a greedy match and places everything after \"findParent\" into the first match, I just added a \"?\" to prevent the greedy match.\n\n{code}\n\/^findParent(\\w+?)(?:By(\\w+))?$\/\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=brenton",
                        "name": "brenton",
                        "displayName": "Brenton Alker",
                        "active": true
                    },
                    "created": "2007-06-26T22:02:46.000+0000",
