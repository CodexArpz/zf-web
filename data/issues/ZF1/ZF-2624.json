                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/23558",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=giallu",
                        "name": "giallu",
                        "displayName": "Gianluca Sforna",
                        "active": true
                    },
                    "body": "Will, Nico, are there any chances this could be reviewed\/pushed to SVN in time for 1.6 gold?",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=giallu",
                        "name": "giallu",
