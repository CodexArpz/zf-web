        "comment": {
            "name": "comment",
            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/40643",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alab",
                        "name": "alab",
                        "displayName": "Christian Albrecht",
                        "active": true
                    },
                    "body": "Sorry, Zend_Form_Element has no method $element->addElementPrefixPath(),\r\nand the difference between $form->addPrefixPath() and $form->addElementPrefixPath()\r\nis that the former adds the Prefix to the Plugin Loader of the given $type,\r\nand the latter calls $element->addPrefixPath() recursively on all SubElements\r\nof the Form and SubForms.\r\nThat means calling the former uses the new Prefix for all new attached elements,\r\nand the latter does not -- it only sets the new Prefix on already attached elements.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alab",
                        "name": "alab",
                        "displayName": "Christian Albrecht",
                        "active": true
                    },
                    "created": "2010-05-22T03:01:40.000+0000",
                    "updated": "2010-05-22T03:01:40.000+0000"
                },
