                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mabe",
                        "name": "mabe",
                        "displayName": "Marc Bennewitz (private)",
                        "active": true
                    },
                    "body": "ZF2 provides a getCapacity method but it looks ok:\r\n{code}\r\n    public function getCapacity(array $options = array())\r\n    {\r\n        $args = new ArrayObject(array(\r\n            'options' => & $options,\r\n        ));\r\n\r\n        try {\r\n            $eventRs = $this->triggerPre(__FUNCTION__, $args);\r\n            if ($eventRs->stopped()) {\r\n                return $eventRs->last();\r\n            }\r\n\r\n            $mem    = wincache_ucache_meminfo ();\r\n            $result = array(\r\n                'free'  => $mem['memory_free'],\r\n                'total' => $mem['memory_total'],\r\n            );\r\n            return $this->triggerPost(__FUNCTION__, $args, $result);\r\n        } catch (\\Exception $e) {\r\n            return $this->triggerException(__FUNCTION__, $args, $e);\r\n        }\r\n    }\r\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=mabe",
                        "name": "mabe",
                        "displayName": "Marc Bennewitz (private)",
                        "active": true
                    },
