        "description": {
            "name": "description",
            "type": "java.lang.String",
            "value": "Zend_Validate_Hostname doesn't handle IDN for .\u0420\u0424, for example \u043f\u043e\u0447\u0442\u0430.\u0440\u0444\r\n\r\n\r\n\r\n        $validator = new Zend_Validate();\r\n        $validator->addValidator(new Zend_Validate_Hostname());\r\n        \r\n        if($validator->isValid('\u043f\u043e\u0447\u0442\u0430.\u0440\u0444')){\r\n            echo \"OK\";\r\n        }else{\r\n            print_r($validator->getMessages());\r\n        }\r\n\r\n\r\n\r\nArray\r\n(\r\n    [hostnameUnknownTld] => '\u043f\u043e\u0447\u0442\u0430.\u0440\u0444' appears to be a DNS hostname but cannot match TLD against known list\r\n    [hostnameInvalidLocalName] => '\u043f\u043e\u0447\u0442\u0430.\u0440\u0444' does not appear to be a valid local network name\r\n)\r\n"
        },
