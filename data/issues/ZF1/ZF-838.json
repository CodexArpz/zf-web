            "value": [
                {
                    "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/comment\/12483",
                    "author": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "The default behaviour of date objects is always that if no value is given, the actual timestamp should be used.\n\nSee date('Ymd'); for example or the new date object from php.\n\nSo we also have to do standard behaviour.\nWhich means that our constructor has also to allow loosing this parameter.\n\nThe following examples work:\nnew Zend_Date();\nnew Zend_Date('ja');\nZend_Date::now();\n\nFixed with SVN 3131 but only for I18N branch.",
                    "updateAuthor": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2007-02-01T00:59:56.000+0000",
                    "updated": "2007-02-01T00:59:56.000+0000"
                }
            ]
