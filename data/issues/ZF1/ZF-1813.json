            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/16170",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=batoon",
                        "name": "batoon",
                        "displayName": "Roman",
                        "active": true
                    },
                    "body": "If I run this code everything is ok:\n{code}\n$mBio = new Litra_Model_Biography();\n$bio = $mBio->fetchAll(array('id_biography = ?' => '00916661186236002296'));\n{code}",
