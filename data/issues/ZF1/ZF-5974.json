                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/29344",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sylvain",
                        "name": "sylvain",
                        "displayName": "Sylvain Filteau",
                        "active": true
                    },
                    "body": "Maybe it would be cleaner to add an interface (Zend_Json_Encodable) with the method toJson ?",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=sylvain",
                        "name": "sylvain",
                        "displayName": "Sylvain Filteau",
