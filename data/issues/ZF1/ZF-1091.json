                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=aheimlich",
                        "name": "aheimlich",
                        "displayName": "Aaron Heimlich",
                        "active": true
                    },
                    "body": "Just so you guys are aware, [jQuery|http:\/\/www.jquery.com] uses \"X-Requested-With: XMLHttpRequest\" ([source|http:\/\/dev.jquery.com\/browser\/trunk\/jquery\/src\/ajax\/ajax.js#L643])",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=aheimlich",
                        "name": "aheimlich",
                        "displayName": "Aaron Heimlich",
                        "active": true
