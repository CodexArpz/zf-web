            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/19164",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alexander",
                        "name": "alexander",
                        "displayName": "Alexander Veremyev",
                        "active": true
                    },
                    "body": "Could you also attach a script you are using to update PDF document?",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=alexander",
                        "name": "alexander",
                        "displayName": "Alexander Veremyev",
                        "active": true
                    },
                    "created": "2008-02-15T14:58:03.000+0000",
                    "updated": "2008-02-15T14:58:03.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/19511",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ljbuesch",
                        "name": "ljbuesch",
                        "displayName": "Logan Buesching",
                        "active": true
                    },
                    "body": "Here is some sample code that will do it:\n<?php\n\nrequire 'Zend\/Loader.php'; \nfunction __autoload($class) \n{\nZend_Loader::loadClass($class); \n}\n\n$pdf=Zend_Pdf::load('original.pdf');\n\n$cover= new Zend_Pdf_Page('A4');\n$font=Zend_Pdf_Font::fontWithName(Zend_Pdf_Font::FONT_TIMES_BOLD);\n$cover->setFont($font,12);\n$cover->drawText(\"HELLO WORLD\",100,100);\n\t\t\narray_unshift($pdf->pages,$cover);\n\n$pdf->save('modified.pdf');\n\necho \"done\";\n\n?>",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=ljbuesch",
                        "name": "ljbuesch",
                        "displayName": "Logan Buesching",
                        "active": true
                    },
                    "created": "2008-02-29T13:48:22.000+0000",
                    "updated": "2008-02-29T13:48:22.000+0000"
                },
