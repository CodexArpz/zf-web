                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=tettema",
                        "name": "tettema",
                        "displayName": "Taylor Ettema",
                        "active": true
                    },
                    "body": "Just to be precise - the warning is thrown out of getFilters when I came across it in 1.6.2",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=tettema",
