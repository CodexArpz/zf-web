            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/33475",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "body": "By default, the Json action and view helpers call exit() immediately after echoing output -- which means that the Firebug log writer is never called.\n\nYou can change this behavior by enabling the suppressExit flag on the helper; when you do so, be sure to immediately return after calling the JSON helper.\n{code}\n$this->_helper->json->suppressExit = true;\n$this->_Helper->json($response);\nreturn;\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "created": "2009-08-15T13:55:41.000+0000",
                    "updated": "2009-08-15T13:55:41.000+0000"
                }
            ]
