        "description": {
            "name": "description",
            "type": "java.lang.String",
            "value": "Zend_Locale_Format::isFloat accepts integers\n\nReproduce Code:\n{code}\nrequire_once 'Zend\/Locale.php';\n\n$locale = new Zend_Locale('en_US');\nif (Zend_Locale_Format::isFloat('122', array('locale' => $locale))) {\n    print \"Float\";\n} else {\n    print \"not a Float\";\n} \n{code}"
