            "type": "com.atlassian.jira.issue.fields.CommentSystemField",
            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/15267",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "body": "'122' as used within your example can be seen as float or integer value.\n\nZend_Locale_Format::isFloat does not check if your input is from the \"type float\" it checks if the content can be seen as float.\nAnd 122 can also be expressed as float, therefor isFloat returns true.\n\nIf you normalize your input with precision 2 you will get returned \"122.00\" which is not an integer but an float value.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=thomas",
                        "name": "thomas",
                        "displayName": "Thomas Weidner",
                        "active": true
                    },
                    "created": "2007-06-17T11:51:23.000+0000",
                    "updated": "2007-06-17T11:51:23.000+0000"
                }
            ]
        }
