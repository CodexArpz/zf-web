            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/47208",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "body": "Can you provide a sample row that we can use to better try and reproduce the issue?\r\n\r\nAlso, have you stripped require_once calls and replaced with autoloading? Are you using an opcode cache?",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=matthew",
                        "name": "matthew",
                        "displayName": "Matthew Weier O'Phinney",
                        "active": true
                    },
                    "created": "2011-06-16T18:31:21.000+0000",
                    "updated": "2011-06-16T18:31:21.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/47209",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=doobywho",
                        "name": "doobywho",
                        "displayName": "Ryan Masten",
                        "active": true
                    },
                    "body": "No opcode cache in my debugging environment currently, although I have tried and it didn't make a difference with this.\r\n\r\nThis particular service call doesn't have any require_once calls.\r\n\r\nI've attached two database table examples. I use an join to link the insurance company table to the state table. The tables are located in two different databases as well.\r\n\r\n",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=doobywho",
                        "name": "doobywho",
                        "displayName": "Ryan Masten",
                        "active": true
                    },
                    "created": "2011-06-16T19:33:30.000+0000",
                    "updated": "2011-06-16T19:33:30.000+0000"
                },
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/47210",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=doobywho",
                        "name": "doobywho",
                        "displayName": "Ryan Masten",
                        "active": true
                    },
                    "body": "Ok, nevermind... I can't attach files. so here you go.\r\n\r\nCREATE TABLE `states` (\r\n  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'State ID',\r\n  `full_name` varchar(45) DEFAULT NULL COMMENT 'Full name of state',\r\n  `abbr` varchar(5) DEFAULT NULL COMMENT 'Abbreviation for state',\r\n  `active` tinyint(1) DEFAULT '1' COMMENT 'Is this state active?',\r\n  PRIMARY KEY (`id`)\r\n) ENGINE=InnoDB AUTO_INCREMENT=63 DEFAULT CHARSET=latin1 ROW_FORMAT=COMPACT COMMENT='State link table';\r\n\r\ninsert into `states` values('1','TEXAS','TX','1');\r\n\r\nCREATE TABLE `insurance_company` (\r\n  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,\r\n  `Carrier Code` varchar(6) DEFAULT NULL,\r\n  `Plan Type` varchar(3) DEFAULT NULL,\r\n  `Plan name` varchar(60) DEFAULT NULL,\r\n  `Address 1` varchar(60) DEFAULT NULL,\r\n  `Address 2` varchar(45) DEFAULT NULL,\r\n  `City` varchar(45) DEFAULT NULL,\r\n  `Zip` varchar(10) DEFAULT NULL,\r\n  `States_id` int(11) NOT NULL,\r\n  PRIMARY KEY (`id`,`States_id`),\r\n  KEY `States_id` (`States_id`)\r\n) ENGINE=InnoDB AUTO_INCREMENT=1485 DEFAULT CHARSET=latin1 ROW_FORMAT=COMPACT COMMENT='Insurance Companies';\r\n\r\ninsert into `insurance_company` values('1','AETNA','PI','AETNA HMO','PO BOX 981106',null,'EL PASO','79998','1');\r\n\r\nMy database of insurance companies has about 1600 rows.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=doobywho",
                        "name": "doobywho",
                        "displayName": "Ryan Masten",
                        "active": true
                    },
