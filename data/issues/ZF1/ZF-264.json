                },
                {
                    "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/comment\/13517",
                    "author": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=garth.gillespie",
                        "name": "garth.gillespie",
                        "displayName": "Garth Gillespie",
                        "active": true
                    },
                    "body": "What is the deal with this getting continually pushed back.  Do the developers disagree with the proposed fix?",
                    "updateAuthor": {
                        "self": "http:\/\/framework.zend.com\/issues\/rest\/api\/latest\/user?username=garth.gillespie",
