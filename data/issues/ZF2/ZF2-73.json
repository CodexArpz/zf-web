            "value": [
                {
                    "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/comment\/49068",
                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=singles",
                        "name": "singles",
                        "displayName": "Rados\u0142aw Benkel",
                        "active": true
                    },
                    "body": "At the end of setStatusCode(), code is casted to integer. \r\n\r\n{code:title=Response.php|borderStyle=solid}\r\npublic function setStatusCode($code)\r\n{\r\n    $const = get_called_class() . '::STATUS_CODE_' . $code;\r\n    if (!is_numeric($code) || !defined($const)) {\r\n        $code = is_scalar($code) ? $code : gettype($code);\r\n        throw new Exception\\InvalidArgumentException(sprintf(\r\n            'Invalid status code provided: \"%s\"',\r\n            $code\r\n        ));\r\n    }\r\n    $this->statusCode = (int) $code;\r\n    return $this;\r\n}\r\n{code}",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=singles",
                        "name": "singles",
                        "displayName": "Rados\u0142aw Benkel",
                        "active": true
                    },
                    "created": "2011-11-11T19:22:05.000+0000",
                    "updated": "2011-11-11T19:22:05.000+0000"
                }
            ]
        }
