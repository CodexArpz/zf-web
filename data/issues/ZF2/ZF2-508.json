        "description": {
            "name": "description",
            "type": "java.lang.String",
            "value": "\\Zend\\View\\Helper\\ServerUrl does not take into account when the request is coming from a proxy server. Here is that patch.\r\n\r\n{code}\r\n        if (isset($_SERVER['HTTP_X_FORWARDED_HOST']) && !empty($_SERVER['HTTP_X_FORWARDED_HOST'])) {\r\n            $host = $_SERVER['HTTP_X_FORWARDED_HOST'];\r\n            if (strpos($host, ',')) {\r\n                \/\/ See Symfony ticket #3842 (http:\/\/trac.symfony-project.org\/ticket\/3842)\r\n                $hosts = explode(',', $host);\r\n                $host = array_pop($hosts);\r\n            }\r\n            $this->setHost($host);\r\n        } elseif (isset($_SERVER['HTTP_HOST']) && !empty($_SERVER['HTTP_HOST'])) {\r\n{code}"
        },
