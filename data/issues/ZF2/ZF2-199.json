                    "author": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=omerida",
                        "name": "omerida",
                        "displayName": "Oscar Merida",
                        "active": true
                    },
                    "body": "Thanks You! When the fix is merged I will test it too.",
                    "updateAuthor": {
                        "self": "http:\/\/fw02.zend.com\/issues\/rest\/api\/latest\/user?username=omerida",
                        "name": "omerida",
                        "displayName": "Oscar Merida",
