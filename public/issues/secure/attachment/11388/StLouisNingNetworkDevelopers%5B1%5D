<script type='text/javascript'>
tooltip('mylo_tooltip','dialog_mylo', {width: '400px'});

function synd_view(w, vis) {
    if (w == 'me') {
        var obj = element('syndication_0');
    } else if(w=='world') {
        var obj = element('syndication_2');
    } else {
        var obj = element('syndication_1');
    }

    if (vis) {
        obj.style.display = 'block';
    } else {
        obj.style.display = 'none';
    }
    
}

function synd_hover(w, vis) {
    if (w == 'me') {
        var obj = element('clicker_0');
    } else if (w =='world'){
        var obj = element('clicker_2');
    }
    else {
        var obj = element('clicker_1');
    }

    
    if (vis) {
        obj.style.backgroundImage='url(http://www.mybloglog.com/buzz/images/rss_subscribe_white_o.gif)';
    } else {
        obj.style.backgroundImage='url(http://www.mybloglog.com/buzz/images/rss_subscribe_white_u.gif)';
    }
    
    
}

</script>
    
