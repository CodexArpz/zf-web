        <script type="text/javascript" charset="utf-8">
        var loaderObj;
        var loaded = false;

        function loadNewWith (type) {
          if(loaderObj != null) {
            loaderObj.stop();
            loaded = false;
          }
          loaderObj = new Ajax.PeriodicalUpdater('tab_content_nwm_all', 'http://www.mybloglog.com/buzz/members/mem_newwithme_ajax.php?m=StLouisNingNetworkDevelopers&w=' + type, {
            method: 'get', frequency: 30, decay: 2, onLoading: function(transport) {
              if(loaded == false) {
                Element.hide('tab_content_nwm_all');
                if($("tab_nwm").className == "tab_tabon") {
                    Element.show('nwm_loading');
                }
              }
            },
            onLoaded: function(transport) {
              if ($('tab_nwm').className == "tab_tabon") {
                Element.hide('nwm_loading');
                Element.show('tab_content_nwm_all');
              }
              loaded = true;
            }
          });

        }
        loadNewWith('me');
      </script>
