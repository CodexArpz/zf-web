<script type='text/javascript'>

var effect_1 = null;


disp = Array();
disp['u'] = Array();
disp['d'] = Array();



var s_actives = Array();

function s_restart(id, flag) {
        if (flag) { s_actives[id][3] = flag; }

        s_inf(s_actives[id][0], s_actives[id][1], s_actives[id][2], s_actives[id][3]);
}

function s_setstatus(id, status) {
        s_actives[id][4] = status;
}

function s_getstatus(id) {
        if (s_actives[id]) return s_actives[id][4];
}

function s_inf(c, p, evt, t) {
        var obj = $(p);
        var id = c;
        var status = s_getstatus(id);

        if (!status) {
                s_actives[id] = Array(c, p, evt, t, 'closed');
                status = 'closed';
        }

        if (t == 'on') {
                for (var oo in s_actives) {
                        if (oo != id) {
                                switch (s_getstatus(oo)) {
                                        case 'opening':
                                        case 'open':
                                                setTimeout("s_restart('"+oo+"', 'off')", 50);
                                }
                        }

                }

                if (status != 'closed') return;

                Effect.SlideDown(id, {duration: 0.3});

                s_setstatus(id, 'opening');

                setTimeout("s_setstatus('"+id+"', 'open')", 500);

        } else {
                if (status != 'open') {
                //      setTimeout("s_restart('"+id+"')", 1000);
                        return;
                }

                Effect.SlideUp(id, {duration: 0.3});

                s_setstatus(id, 'closing');

                setTimeout("s_setstatus('"+id+"', 'closed')", 300);
        }
}

var olddivid='';
function add_s_inf(divid,fieldid) {
//      alert(olddivid);
        if(olddivid)
                document.getElementById(olddivid).style.display="none";
        document.getElementById(divid).style.display = "inline";

        document.getElementById(fieldid).select();
        document.getElementById(fieldid).focus();
                olddivid=divid;
}

function hide_form(divid)
{

        document.getElementById(divid).style.display = "none";

}



function sendGetRequest(url) {
    var xhr;

    xhr = YAHOO.util.Connect.asyncRequest("GET", url, {
        success: function (o) {
            if (typeof o.responseText != "undefined")
                return o.responseText;
        },

        failure: function (o) {
            if (typeof o != "undefined" && o.status && o.statusText)
                return "Failure: " +
                        "server returned: " + o.status + ": " + o.statusText;
        }
    });

    if (!xhr) {
        return false;
    }

 }
 
function sendPostRequest(url,id) {
    var allelems,
        data,
        el,
        i,
        n,
        formname,
        xhr;
/*
    formname="form_"+id;
    allelems = document.forms[formname].elements; */
 textbox_id ='name_' + id ;
    data = "";


if(!(document.getElementById(textbox_id).value))

                {
                        document.getElementById("add_text_"+id).style.display="inline";
                        return;
                }
            else
                {
                        document.getElementById("add_text_"+id).style.display="none";
                }

    data = "field_id=" + document.getElementById(textbox_id).name + "&field_value=" +  document.getElementById(textbox_id).value + "&";

/*
    for (i = 0, n = allelems.length; i < n; i++)
    {
            data += allelems[i].name + "=" + allelems[i].value + "&";
            if(!(allelems[i].value))

                {
                        document.getElementById("add_text_"+id).style.display="inline";
                        return;
                }
            else
                {
                        document.getElementById("add_text_"+id).style.display="none";
                }



    }
*/
    data +="h_chk=8e47d37691af455435e2092edfa42a5d";
        url += "?" + data;
    xhr = YAHOO.util.Connect.asyncRequest("GET", url, {
        success: function (o) {
		var op = eval( ' ( ' + o.responseText + ' ) ' );
            if(op['status'] == 1)
                {
                        document.getElementById("add_"+id).innerHTML = "<font color='red' style='font-weight:normal;'>Successfully added to your profile.</font>";
                        hide_form("addspan_"+id);
                }
            else
                {
document.getElementById("add_text_"+id).innerHTML = "<font color='red' style='font-weight:normal;'>"+op['message']+"</font>";

                     document.getElementById("add_text_"+id).style.display="inline";

                }


        },

        failure: function (o) {
            if (typeof o != "undefined" && o.status && o.statusText)
                document.getElementById("add_"+id).INnerHTML = "Failure: " +
                        "server returned: " + o.status + ": " + o.statusText;
        }
    });

    if (!xhr) {
        alert("Giving up. Your web browser does not support XMLHttpRequest");
        return;
    }
}

var dispCount= new Array();
var dispMap= new Array();

var x=0;


function sendPostDelRequest(url,id) {
    var allelems,
        data,
        el,
        i,
        n,
        formname,
        xhr;

    formname="form_del_"+id;
    allelems = document.forms[formname].elements;
    data = "";
    for (i = 0, n = allelems.length; i < n; i++)
    {
            data += allelems[i].name + "=" + allelems[i].value + "&";

    }

    data +="h_chk=8e47d37691af455435e2092edfa42a5d";
    xhr = YAHOO.util.Connect.asyncRequest("POST", url, {
        success: function (o) {


                {
                      var row_id=dispMap[id];

                      if(--dispCount[row_id])
                              document.getElementById("descspan_"+id).innerHTML = "";//<font color='red' style='font-weight:normal;'>"+o.responseText+"</font>";
                      else
                        {
                             document.getElementById("main_block"+row_id).style.display = "none";

                        }
                }


        },

        failure: function (o) {
            if (typeof o != "undefined" && o.status && o.statusText)
                alert( "Sorry, no donut for you. Cannot delete this entry right now. Please try later.");

        }
    }, data);

    if (!xhr) {
        alert("Giving up. Your web browser does not support XMLHttpRequest");
        return;
    }
}

function vitality_cross(w, id, o, no) {
    mbl_rr_rdy = false;
    var img =  document.getElementById(id);
    if (!img) {
        return;
    }
    if (w == "s") {
        if (!no) {
            img.style.display = "";
        }
        if (o.id == id) {
            mbl_rr_rdy = true;
        }
    } else {
        if (!no) {
            img.style.display = "none";
        }
    }
}

function vitality_hide(id) {
        var answer = confirm("Are you sure you want to delete this update from this list?");
        if (!answer)
                return false;
        sendPostDelRequest('http://www.mybloglog.com/buzz/members/mem_vitality_delete_service.php',id);


}

function redirUser($url)
{
 location.href=$url;
}
