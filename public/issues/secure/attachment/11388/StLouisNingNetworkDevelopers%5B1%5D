<script language="javascript">

function element(id){
 if(document.getElementById != null){return document.getElementById(id);}
 if(document.all != null){return document.all[id];}
 if(document.layers != null){return document.layers[id];}
 return null;
}

var cur_show_msg_val = '';
function show_msg_tog(obj) {
  if(obj && obj.value != cur_show_msg_val) {
    window.location = 'http://www.mybloglog.com/buzz/' + 'members/mem_msg_opt.php?s=' + obj.value + '&ref=' + escape(window.location);
    cur_show_msg_val = obj.value;
  }
}

function mbl_gpos(obj){
  var curleft=curtop=0;

  if(obj.offsetParent){
    curleft=obj.offsetLeft;
    curtop=obj.offsetTop;

    while(obj=obj.offsetParent){
      curleft+=obj.offsetLeft;
      curtop+=obj.offsetTop;
    }
  }

  return[curleft,curtop];
}

function tag_toggle() {
 if (!element('tag_info_container')) return;
 element('tag_info_container').style.display = 'none';
}

var last_tag;
function tag_info(tagid,tog) {
 var tp = mbl_gpos(element('mem_comm_tags'));
 element('tag_info_container').style.top = (tp[1] + 50) + 'px';

 if(last_tag)
 {
  var box = 'tbox'+last_tag;
  var boxt1 = 'tag1'+last_tag;
  if(element(box)) { element(box).style.display='none'; }
  element('tag_info_container').style.display = 'none';
  if(element(boxt1)) { element(boxt1).style.textDecoration='none'; }
 }
 element('tag_message').style.display = 'none';
 element('tag_info_container').style.display = 'block';
 var t = 'tbox'+tagid;
 element(t).style.display='block';
 var tl = 'tag1'+tagid;
 element(tl).style.textDecoration='underline';
 last_tag = tagid;

}

</script>
