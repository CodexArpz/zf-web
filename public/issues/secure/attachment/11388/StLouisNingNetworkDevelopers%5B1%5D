   <script>
        function toggle_vis(obj, vis) {
            obj = element(obj);

            if (vis) {
                obj.style.display = 'block';
            } else {
                obj.style.display = 'none';
            }
        }

        function toggle_nwm(obj) {

            toggle_vis('clicker_0_wrap', false);
            toggle_vis('clicker_1_wrap', false);
            toggle_vis('clicker_2_wrap', false);

            toggle_vis('syndication_0', false);
            toggle_vis('syndication_1', false);
            toggle_vis('syndication_2', false);

            element('vit_me').className = 'off';
            element('vit_everyone').className = 'off';
            element('vit_world').className = 'off';

            switch (obj) {
                case 'me':
                    loadNewWith('me');
                    element('vit_me').className = 'on';
                    element('nwm_mw_in').checked = true;
                    toggle_vis('clicker_0_wrap', true);
                    break;

                case 'neighborhood':
                    loadNewWith('my');
                    element('vit_everyone').className = 'on';
                    element('nwm_mw_ne').checked = true;
                    toggle_vis('clicker_1_wrap', true);
                    break;

                case 'world':
                    loadNewWith('world');
                    element('vit_world').className = 'on';
                    element('nwm_mw_nw').checked = true;
                    toggle_vis('clicker_2_wrap', true);
                    break;
            }
        }

        function toggle_tab(tab) {
            toggle_vis('tab_content_nwm', false);
            toggle_vis('tab_content_hot', false);
            toggle_vis('tab_content_msg', false);
            toggle_vis('tab_content_nwm_all',false);

            element('tab_nwm').className='tab_taboff';
            element('tab_hot').className='tab_taboff';
            element('tab_msg').className='tab_taboff';

            switch (tab) {
                case 'nwm':
                    toggle_vis('tab_content_nwm', true);
                    toggle_vis('tab_content_nwm_all',true);
                    element('tab_nwm').className='tab_tabon';
                    break;

                case 'hot':
                    toggle_vis('tab_content_hot', true);
                    element('tab_hot').className='tab_tabon';
                    break;

                case 'msg':
                    toggle_vis('tab_content_msg', true);
                    element('tab_msg').className='tab_tabon';
                    break;

            }
            
                    
        }
        //onmouseover='this.className="tab_tabhover"' onmouseout='this.className="tab_taboff"'
        function tab_hover(tab, flag) {
            if (flag) {

            } else {

            }
        }

  </script>




