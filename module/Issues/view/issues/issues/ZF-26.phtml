$feed = Zend_Feed::import('<a href="http://maine.craigslist.org/web/index.rss">http://maine.craigslist.org/web/index.rss</a>');

foreach ($feed as $item) { echo 'title: ', $item-&gt;title(), "\n"; echo 'link: ', $item-&gt;link(), "\n\n"; }
</code></pre>

